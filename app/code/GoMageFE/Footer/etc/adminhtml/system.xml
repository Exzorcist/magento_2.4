<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="gomagefe" translate="label" sortOrder="10">
            <label>GoMageFE</label>
        </tab>
        <section id="gomagefe_footer" translate="label" type="text" sortOrder="100" showInDefault="1"
                    showInWebsite="1" showInStore="1">
            <label>GoMageFE Footer</label>
            <tab>gomage_tab</tab>
            <resource>Aureatelabs_LoginAsCustomer::config</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1"
                    showInStore="1">
                <label>General</label>
                <field id="gomagefe_footer_enabled" translate="label" type="select" showInDefault="1" canRestore="1"
                        showInWebsite="1" showInStore="1">
                    <label>Enabled</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>

    </system>
</config>
